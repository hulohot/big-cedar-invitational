[Unit]
Description=HuloPredict Prediction Market Server
After=network.target

[Service]
Type=simple
User=ebrugger
WorkingDirectory=/home/ebrugger/.openclaw/workspace/big-cedar-invitational/backend
ExecStart=/usr/bin/node server.js
Restart=always
RestartSec=10
Environment=NODE_ENV=production

[Install]
WantedBy=multi-user.target
